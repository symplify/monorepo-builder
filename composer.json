{
    "name": "symplify/monorepo-builder",
    "description": "Prefixed version of Not only Composer tools to build a Monorepo.",
    "license": "MIT",
    "require": {
        "php": ">=7.2"
    },
    "bin": [
        "bin/monorepo-builder"
    ],
    "extra": {
        "branch-alias": {
            "dev-main": "10.3-dev"
        }
    },
    "autoload": {
        "files": [
            "bootstrap.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "phpstan/extension-installer": true
        }
    }
}
